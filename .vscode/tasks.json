{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Make all",
			"type": "shell",
			"command": "make",
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Make Clean & Build",
			"type": "shell",
			"command": "make clean && make",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}