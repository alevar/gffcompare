    "version": "0.2.0",
    "configurations": [
       {
        "name": "launch trmap",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}\\trmap",
        "args": ["-t", "o.tmap", "-o", "o.jtab", "-J", "examples/annotation.gff", "examples/transcripts.gtf"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            }
          ],
          "preLaunchTask": "Make all"
        },
        {
            "name": "launch gffcompare",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\gffcompare",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
                }
              ],
              "preLaunchTask": "Make all"
            }
      
    ]
}